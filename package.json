{
  "name": "nuxt3-starter",
  "description": "Nuxt 3 Awesome Starter",
  "version": "1.0.0",
  "license": "MIT",
  "author": "mukundshah",
  "repository": {
    "type": "git",
    "url": "https://github.com/mukundshah/nuxt3-starter"
  },
  "engines": {
    "node": ">=16.10.0",
    "pnpm": ">=7.10.0"
  },
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint --ext \".ts,.js,.vue\" --ignore-path .gitignore .",
    "lint:fix": "eslint --fix --ext \".ts,.js,.vue\" --ignore-path .gitignore .",
    "prepare": "husky install",
    "start": "node .output/server/index.mjs"
  },
  "lint-staged": {
    "**/*.{js,ts,vue,html}": [
      "pnpm lint:fix"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@formkit/nuxt": "^0.17.2",
    "@formkit/themes": "^0.17.2",
    "@headlessui/tailwindcss": "^0.1.3",
    "@heroicons/vue": "^2.0.18",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@nuxtjs/eslint-module": "4.1.0",
    "@nuxtjs/google-fonts": "3.0.1",
    "@nuxtjs/tailwindcss": "^6.7.0",
    "@pinia/nuxt": "^0.4.11",
    "@unlighthouse/nuxt": "^0.8.6",
    "@vueuse/nuxt": "^10.1.2",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.14.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "nuxt": "3.5.1",
    "nuxt-headlessui": "^1.1.4",
    "nuxt-typed-router": "^3.2.2",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.5",
    "typescript": "^5.0.4",
    "vue-tsc": "^1.6.5",
    "@pinia-plugin-persistedstate/nuxt": "^1.1.1"
  }
}
